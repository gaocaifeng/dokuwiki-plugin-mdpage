{
    "name": "mizunashi/dokuwiki-plugin-mdpage",
    "type": "dokuwiki-plugin",
    "description": "DokuWiki Markdown Plugin",
    "keywords": [
        "dokuwiki",
        "plugin",
        "markdown"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mizunashi Mana",
            "email": "mizunashi-mana@noreply.git"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "scripts": {
        "test": "phpunit"
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "cebe/markdown": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35 || ^5.7",
        "squizlabs/php_codesniffer": "^3.2",
        "psy/psysh": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "DokuWiki\\": "src/DokuWiki"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dokuwiki\\Test\\": "tests/DokuWiki/Test"
        }
    }
}
